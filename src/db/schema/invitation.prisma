model Invitation {
    idInvitation String @id @default(cuid())
    idFamily String
    idUserInvited String
    idUserInviter String
    accepted Boolean?

    userInvited User @relation(name: "UserInvited", fields: [idUserInvited], references: [idUser])
    userInviter User @relation(name: "UserInviter", fields: [idUserInviter], references: [idUser])
    family Family @relation(fields: [idFamily], references: [idFamily])
}